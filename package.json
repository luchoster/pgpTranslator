    "moment": "^2.22.1",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "openpgp": "^3.0.11",
    "ramda": "^0.25.0",
    "react": "^16.4.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.4.0",
